{
    "name": "makeabledk/laravel-factory-enhanced",
    "license": "CC-BY-SA-4.0",
    "autoload": {
        "psr-4": {
            "Makeable\\LaravelFactory\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "require": {
        "php": "^7.4",
        "illuminate/database": "^8.0",
        "illuminate/support": "^8.0",
        "fzaninotto/faker": "^1.4"
    },
    "require-dev": {
        "codedungeon/phpunit-result-printer": "^0.28.0",
        "doctrine/dbal": "^2.5",
        "mockery/mockery": "^1.3",
        "laravel/laravel": "^8.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Makeable\\LaravelFactory\\Tests\\": "tests/",
            "Makeable\\LaravelFactory\\Tests\\Database\\Factories\\": "tests/database/factories"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Makeable\\LaravelFactory\\FactoryServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"
    }
}
